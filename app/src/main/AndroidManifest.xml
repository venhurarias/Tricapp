<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techno.tricapp.tricapp"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:replace="@android:icon">
        <activity android:name=".demoactivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".destinationactivity"/>

        <activity android:name=".loginactivity"/>

        <activity android:name=".signupactivity"/>

        <activity android:name=".driverpageactivity"/>

        <activity android:name=".ridedetailactivity"/>

        <activity android:name=".driverconfirmactivity"/>

        <activity android:name=".rideconfirmationactivity"/>

        <activity android:name=".firstactivity"/>

        <activity android:name=".startupactivity"/>
    </application>

</manifest>